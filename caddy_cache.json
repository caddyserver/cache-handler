{
	"admin": {
		"disabled": true
	},
	"apps": {
		"http": {
			"servers": {
				"dev": {
					"listen": [":80"],
					"routes": [
						{
							"match": [{
								"path": ["/cache-default"]
							}],
							"handle": [
								{
									"handler": "cache",
									"default_ttl": 5
								},
								{
									"handler": "static_response",
									"body": "Hello there!"
								}
							]
						},
						{
							"match": [{
								"path": ["/cache-header"]
							}],
							"handle": [
								{
									"handler": "cache"
								},
								{
									"handler": "static_response",
									"headers": {
										"Cache-Control": ["s-maxage=60"]
									},
									"body": "Hello there!"
								}
							]
						}
					]
				}
			}
		}
	}
}
